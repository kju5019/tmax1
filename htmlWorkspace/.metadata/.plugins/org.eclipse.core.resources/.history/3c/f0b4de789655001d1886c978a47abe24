<body>
<script>
function calcSum(n){
	let sum=0;		/* let 변수명 */
	for(let i=1; i<n; i++){
		sum +=i;
	}
	/*	let sum=100;  let은 재선언이 안됨***************************/
	console.log(sum);
}
calcSum(10);
</script>
</body>
